<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MenuButtonView">
        <attr name="mbv_title" format="string"/>
        <attr name="mbv_icon" format="reference" />
        <attr name="mbv_type" format="enum">
            <enum name="cashin" value="0"/>
            <enum name="agentcashin" value="1"/>
            <enum name="scan" value="2"/>
            <enum name="topup" value="3"/>
            <enum name="setup" value="4"/>
            <enum name="support" value="5"/>
            <enum name="notipay" value="6"/>
            <enum name="history" value="7"/>
            <enum name="pin" value="8"/>
            <enum name="other" value="9"/>
        </attr>
        <attr name="mbv_visibility" format="enum">
            <enum name="show" value="0"/>
            <enum name="disable" value="1"/>
            <enum name="hide" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="InformationView">
        <attr name="iv_title" format="string"/>
        <attr name="iv_des" format="string"/>
        <attr name="iv_length_video" format="string"/>
        <attr name="iv_thumbnail" format="reference"/>
    </declare-styleable>

    <declare-styleable name="WidgetTypeInbox">
        <attr name="ti_type" format="enum">
            <enum name="video" value="0"/>
            <enum name="video_vr" value="1"/>
            <enum name="image" value="2"/>
        </attr>
    </declare-styleable>
</resources>